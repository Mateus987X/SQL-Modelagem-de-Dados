SELECT 
    'P |01|SA3010|' + COALESCE(NULLIF(RTRIM(COALESCE(A3_FILIAL, ' ')) + '|' + RTRIM(COALESCE(A3_COD, ' ')), ' '), '|') AS PK_VENDEDOR,
    SA3.A3_COD AS COD_REPRESENTANTE,
    SA3.A3_NOME AS REPRESENTANTE,
    SA3.A3_AJCUSTO AS AJUDA_CUSTO,
    SA3.A3_COMIS AS COMISSAO
FROM SA3010 SA3 
WHERE  
    SA3.D_E_L_E_T_ = ' '
